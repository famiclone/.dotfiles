if [ -f /usr/bin/grc ]; then
 alias gcc="grc --colour=auto gcc"
 alias irclog="grc --colour=auto irclog"
 alias log="grc --colour=auto log"
 alias netstat="grc --colour=auto netstat"
 alias ping="grc --colour=auto ping"
 alias proftpd="grc --colour=auto proftpd"
 alias traceroute="grc --colour=auto traceroute"
fi

# MacOS JavascriptCore
#alias jsc=/System/Library/Frameworks/JavaScriptCore.framework/Versions/Current/Helpers/jsc

# Download youtube mp4 video in best quality
alias ydl="youtube-dl --format 'bestvideo[ext=mp4]+bestaudio[ext=m4a]/mp4' --merge-output-format mp4"
alias ydla="youtube-dl --extract-audio --audio-format mp3 $1"
alias icloud="cd ~/Library/Mobile\ Documents/com~apple~CloudDocs/"
alias v=nvim
alias tmx=tmuxinator
alias la="exa -laFh --git"
alias wget="wget -c"
alias news="newsboat"
alias mpcx="~/.dotfiles/scripts/mpcx"
alias f2mp3='for f in *.flac; do ffmpeg -i "$f" -ab 320k -map_metadata 0 -id3v2_version 3 "${f%.flac}.mp3"; done'

alias cat=bat
alias :q=exit
#alias ls="exa --long --git"

alias nms="find ~/Projects -name "node_modules" -type d -prune -print | xargs du -chs"
